@article{ramanSparseCovaryingUnit2019,
  title = {A Sparse Covarying Unit That Describes Healthy and Impaired Human Gut Microbiota Development},
  author = {Raman, Arjun S. and Gehrig, Jeanette L. and Venkatesh, Siddarth and Chang, Hao-Wei and Hibberd, Matthew C. and Subramanian, Sathish and Kang, Gagandeep and Bessong, Pascal O. and Lima, Aldo A.M. and Kosek, Margaret N. and Petri, William A. and Rodionov, Dmitry A. and Arzamasov, Aleksandr A. and Leyn, Semen A. and Osterman, Andrei L. and Huq, Sayeeda and Mostafa, Ishita and Islam, Munirul and Mahfuz, Mustafa and Haque, Rashidul and Ahmed, Tahmeed and Barratt, Michael J. and Gordon, Jeffrey I.},
