}

@article{huangChickenGutMetagenome2018,
  title = {The Chicken Gut Metagenome and the Modulatory Effects of Plant-Derived Benzylisoquinoline Alkaloids},
  author = {Huang, Peng and Zhang, Yan and Xiao, Kangpeng and Jiang, Fan and Wang, Hengchao and Tang, Dazhi and Liu, Dan and Liu, Bo and Liu, Yisong and He, Xi and Liu, Hua and Liu, Xiubin and Qing, Zhixing and Liu, Conghui and Huang, Jialu and Ren, Yuwei and Yun, Long and Yin, Lijuan and Lin, Qian and Zeng, Cheng and Su, Xiaogang and Yuan, Jingyang and Lin, Li and Hu, Nanxi and Cao, Hualiang and Huang, Sanwen and Guo, Yuming and Fan, Wei and Zeng, Jianguo},
