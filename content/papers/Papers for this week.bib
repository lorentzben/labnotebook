@article{karasawaSignificantRoleNitrogen1999,
  title = {Significant Role of the Nitrogen Recycling System through the Ceca Occurs in Protein-Depleted Chickens},
  author = {Karasawa, Yutaka},
  date = {1999},
  journaltitle = {Journal of Experimental Zoology},
  volume = {283},
